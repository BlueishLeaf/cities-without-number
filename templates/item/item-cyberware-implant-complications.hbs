{{!-- Implant Complications --}}
<ol class="items-list">
    <li class="item flexrow items-header">
        <div class="item-name">Implant Complications</div>
        <div class="item-controls">
            <a class="item-control item-create" title="Create Implant Complication" data-type="implantComplication"><i
                    class="fas fa-plus"></i> Add</a>
        </div>
    </li>
    {{#each implantComplication as |item id|}}
        <li class="item flexrow" data-item-id="{{item._id}}">
            <div class="item-name">
                <div class="item-image">
                    <a class="rollable" data-roll-type="implantComplication"><img class="rollable-img"
                                                                                  src="{{item.img}}"
                                                                                  title="{{item.name}}" width="24"
                                                                                  height="24"/></a>
                </div>
                <h4 class="expandable">{{item.name}}</h4>
            </div>
            <div class="item-controls">
                <a class="item-control item-edit" title="Edit Implant Complication"><i class="fas fa-edit"></i></a>
                <a class="item-control item-delete" title="Delete Implant Complication"><i class="fas fa-trash"></i></a>
            </div>
        </li>
        <div class="item-description hidden">{{{item.system.description}}}</div>
    {{/each}}
</ol>