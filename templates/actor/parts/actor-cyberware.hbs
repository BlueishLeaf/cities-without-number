<div class="heading-resource">
  <label class="resource-label" for="system.cyberwareMaintenanceCost">Monthly Maintenance Cost</label>
  <input type="text" name="system.cyberwareMaintenanceCost" value="{{system.cyberwareMaintenanceCost}}"  data-dtype="Number"/>
</div>
<hr>
<ol class="items-list">
  <li class="item flexrow items-header">
    <div class="item-name">Body Ware</div>
    <div class="item-stats">Effect</div>
    <div class="item-stats" title="System Strain">Sys. Strain</div>
    <div class="item-controls">
      <a class="item-control item-create" title="Create Body Ware" data-type="cyberware" data-subType="body"><i class="fas fa-plus"></i> Add</a>
    </div>
  </li>
  {{#each body as |bodyWare id|}}
  <li class="item flexrow" data-item-id="{{bodyWare._id}}">
    <div class="item-name">
      <div class="item-image">
        <a class="rollable" data-roll-type="cyberware"><img class="rollable-img" src="{{bodyWare.img}}" title="{{bodyWare.name}}" width="24" height="24"/></a>
      </div>
      <h4 class="expandable">{{bodyWare.name}}</h4>
    </div>
    <div class="item-stats">
      <h4>{{bodyWare.system.effect}}</h4>
    </div>
    <div class="item-stats">
      <h4>{{bodyWare.system.systemStrain}}</h4>
    </div>
    <div class="item-controls">
      <a class="item-control item-edit" title="Edit Body Ware"><i class="fas fa-edit"></i></a>
      <a class="item-control item-delete" title="Delete Body Ware"><i class="fas fa-trash"></i></a>
    </div>
  </li>
  <div class="item-description hidden">{{{bodyWare.system.description}}}</div>
  {{/each}}
</ol>

